<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.view.moderator.ModeratorViewController">
   <children>
      <Label layoutX="256.0" layoutY="14.0" text="Reports">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="reportTable" layoutX="2.0" layoutY="60.0" prefHeight="280.0" prefWidth="594.0">
        <columns>
          <TableColumn fx:id="reportFromColumn" prefWidth="82.39996337890625" text="Report From" />
          <TableColumn fx:id="itemIDColumn" prefWidth="74.4000244140625" text="Item ID" />
            <TableColumn fx:id="accountColumn" prefWidth="82.39996337890625" text="Account ID" />
            <TableColumn fx:id="feedbackItemColumn" prefWidth="107.20001220703125" text="Item Feedback ID" />
            <TableColumn fx:id="dateColumn" prefWidth="116.00003662109373" text="Date" />
        </columns>
      </TableView>
      <HBox layoutX="-1.0" layoutY="339.0" prefHeight="61.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onSeeReport" prefHeight="35.0" text="See Report">
               <HBox.margin>
                  <Insets left="45.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onDelete" prefHeight="35.0" text="Delete ">
               <HBox.margin>
                  <Insets left="45.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onArchive" prefHeight="35.0" text="Archive">
               <HBox.margin>
                  <Insets left="45.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onChat" prefHeight="35.0" text="Chat with">
               <HBox.margin>
                  <Insets left="45.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onMyAccount" prefHeight="35.0" text="Messages">
               <HBox.margin>
                  <Insets left="45.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
